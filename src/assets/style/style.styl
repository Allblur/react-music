html,body
	margin 0
	padding 0
	background-color #fff
	.block
		display block
	.hide
		display none

body
	line-height 1.5em
	background-color #fff
	font-family "Helvetica Neue",Helvetica,Roboto,Arial,"PingFang SC","Noto Sans CJK SC","Source Han Sans CN","Lantinghei SC","Microsoft Yahei","Hiragino Sans GB","WenQuanYi Micro Hei",sans-serif
	font-size 14px
	color #2a2a2a

ul,ol,li
	list-style-type none

img,button,input,textarea,select
	border none
	resize none

ul,ol,li,div,p,img,button,input,textarea,select,form,header,footer,section,aside,article,nav,hgroup,figure,figcaption,h1,h2,h3,h4,h5,h6,dl,dt,dd,bl,ockquote,legend,table,th,td,fieldset,menu,pre
	padding 0
	margin 0

a,a:hover
	color #666
	text-decoration none
	cursor pointer

.wrapper
	width 960px
	margin 0 auto
.gra-btn
	border 1px solid #dadada !important
	background-color #f6f6f6 !important
	color #2a2a2a !important
.gre-btn
	border 1px solid #69b219 !important
	background-color #69b219 !important
	color #fff !important
.red-btn
	border 1px solid #db4437 !important
	background-color #db4437 !important
	color #fff !important
.mtem1
	margin-top 1em
.mtpx30
	margin-left 30px
.pab65
	padding-bottom 65px
.pagination
	height 40px
	ul
		height 40px
		display flex
		align-items center
		justify-content center
		li
			display inline-block
			margin 0 6px
			span
				display inline-block
				padding 5px 10px
				border 1px #e1e1e1 solid
				border-radius 4px
				background-color #fff
				cursor pointer
		.active
			span
				border-color #f30
				background-color #f50
				color #fff
		.on
			span
				border-color #a1a1a1
				background-color #a9a9a9
@font-face 
	font-family "iconfont"
	src url('//at.alicdn.com/t/font_b3mvxdr24ubit3xr.eot?t=1478753378273')
	src url('//at.alicdn.com/t/font_b3mvxdr24ubit3xr.eot?t=1478753378273#iefix') format('embedded-opentype'),
	url('//at.alicdn.com/t/font_b3mvxdr24ubit3xr.woff?t=1478753378273') format('woff'),
	url('//at.alicdn.com/t/font_b3mvxdr24ubit3xr.ttf?t=1478753378273') format('truetype'),
	url('//at.alicdn.com/t/font_b3mvxdr24ubit3xr.svg?t=1478753378273#iconfont') format('svg')

@font-face
	font-family 'billabongregular'
	src url('https://cdn.rawgit.com/milktronics/beaglegr.am/master/public/fonts/billabong-webfont.eot')
	src url('https://cdn.rawgit.com/milktronics/beaglegr.am/master/public/fonts/billabong-webfont.eot?#iefix') format('embedded-opentype'),
	url('https://cdn.rawgit.com/milktronics/beaglegr.am/master/public/fonts/billabong-webfont.woff') format('woff'),
	url('https://cdn.rawgit.com/milktronics/beaglegr.am/master/public/fonts/billabong-webfont.ttf') format('truetype'),
	url('https://cdn.rawgit.com/milktronics/beaglegr.am/master/public/fonts/billabong-webfont.svg#billabongregular') format('svg')


.iconfont
	font-family "iconfont" !important
	font-size 16px
	font-style normal
	-webkit-font-smoothing antialiased
	-webkit-text-stroke-width 0.2px
	-moz-osx-font-smoothing grayscale
.waiting
	margin 0 auto
	height 160px
	width 298px
	border 1px solid #dfdfdf
	box-shadow 0px 1px 2px 0px rgba(158,158,158,0.2)
	text-align center
	font-size 10px
	div
		background-color #00b2ff
		height 60%
		width 6px
		margin-left 8px
		margin-top 10%
		display inline-block
		border-radius 10px
		-webkit-animation stretchdelay 1.3s infinite ease-in-out
		animation stretchdelay 1.3s infinite ease-in-out
	.rect2
		-webkit-animation-delay -1.1s
		animation-delay -1.1s
	.rect3
		-webkit-animation-delay -1.0s
		animation-delay -1.0s
	.rect4
		-webkit-animation-delay -0.9s
		animation-delay -0.9s
	.rect5
		-webkit-animation-delay -0.8s
		animation-delay -0.8s
@-webkit-keyframes stretchdelay
	0%
		-webkit-transform scaleY(0.4)
	20%
		-webkit-transform scaleY(1.0)
	40%
		-webkit-transform scaleY(0.4)
	100%
		-webkit-transform scaleY(0.4)
@keyframes stretchdelay
	0%
		transform scaleY(0.4)
		-webkit-transform scaleY(0.4)
	20%
		transform scaleY(1.0)
		-webkit-transform scaleY(1.0)
	40%
		transform scaleY(0.4)
		-webkit-transform scaleY(0.4)
	100%
		transform scaleY(0.4)
		-webkit-transform scaleY(0.4)
@-webkit-keyframes playingicons
	0%
		height 20px
	20%
		height 30px
	40%
		height 20px
	100%
		height 10px
@keyframes playingicons
	0%
		height 20px
	20%
		height 30px
	40%
		height 20px
	100%
		height 10px
.header
	height 65px
	background-color #db4437
	box-shadow 0 -2px 8px 4px rgba(10,10,10,.4)
	.logo
		width 150px
		height 65px
		line-height 65px
		float left
		font-family "billabongregular"
		font-size 36px
		font-weight normal
		font-style normal
		color #f1f1f1
		cursor pointer
	.header-nav
		margin-left 125px
		overflow hidden
		li
			display inline-block
			a
				display inline-block
				height 65px
				line-height 65px
				padding 0 10px
				color #d9d9d9
		.active
			a
				background-color #3a3a3a
				color #f3f3f3
.search-form
	float right
	height 30px
	width 300px
	margin-top 18px
	.search-wrap
		position relative
		input
			width 288px
			height 20px
			line-height 20px
			padding 4px 5px
			background-color #fff
			border 1px #e9e9e9 solid
			border-radius 4px
			outline none
		.search-btn
			display block
			position absolute
			right 0
			top 0
			background-color #e9e9e9
			padding 3px 12px
			border 1px solid transparent
			border-radius 4px
			cursor pointer
				
.playlist-category
	margin-top 1em
	ul
		margin-left -1em
		li
			display inline-block
			margin-left 1em
			margin-bottom 1em
			height 35px
			span
				display inline-block
				padding 0 10px
				height 33px
				line-height 33px
				border-radius 5px
				cursor pointer
.playlist-item
	ul
		margin-left -30px